{
  "include": [
    "./src/**/*.ts",
    "./test/**/*.ts",
  ],
  "compilerOptions": {
    "target": "ES2022",
    "module": "commonjs",  
    "moduleResolution": "node",
    "baseUrl": ".",                        
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true                   
  }
}
